<script src="../assets/dist/js/bootstrap.bundle.min.js"></script>


<script>
  /* Populates carousel with YouTube videos */
  fetch('https://youtube.googleapis.com/youtube/v3/playlistItems?part=snippet%20&maxResults=5&playlistId=UUXF6DMVLIVRr2OQAqyfEGeg&key=AIzaSyC0UGVBMDSnfW_42iL6qRV0SjAo3Lk6JX8')
        .then(res =>  res.json())
        .then(data => {

          //set current carousel item to active 
          let isActive = true; 

          data.items.forEach(curr => {
              vidTitle = curr.snippet.title;
              vidURL = 'https://www.youtube.com/watch?=v'+ curr.snippet.resourceId.videoId;
              thumbnailURL = curr.snippet.thumbnails.medium.url;
              
          

              //create a markup to show video title and url 
              //blank so that video is opened in a new tab
              markup = `
                  <div class= carousel-item ${isActive ? 'active' : ''}">
                  <a href='${vidURL}' target='_blank'>
                      <img src='${thumbnailURL}' alt='${vidTitle}' class="d-block w-100" style="height: 300px; object-fit: cover;">
                      <div class= carousel-caption>
                        <p>${vidTitle}</p>
                      </div>
                  </a>
                  </div>`;
              //add markup into front end 
              document.querySelector('.carousel-inner').insertAdjacentHTML('beforeend', markup);
              isActive = false;

          });
        });


  /*  Opens iFrame Popups on button click */
  document.querySelectorAll('.btn-secondary').forEach(button => {
    button.addEventListener('click', function() {
      const animal = this.getAttribute('img-id');
      const popup = document.getElementById('popup');
      const popupContent = document.getElementById('popup-content');

      // Set the content of the popup based on data-animal
      popupContent.innerHTML = `<iframe id="animal-webpage" src="${animal}.html"></iframe><button id="close-btn">X</button>`;

      // Show the popup
      popup.classList.remove('hidden-popup');
      
      // Close button event listener
      document.getElementById('close-btn').addEventListener('click', function() {
        popup.classList.add('hidden-popup');
      });
    });
  });
</script>




    </body>
</html>
